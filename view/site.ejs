<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="//unpkg.com/purecss@1.0.0/build/base-min.css">
</head>

<body>
    <div class="pure-g">
        <div class="pure-u-1-3">
        </div>
        <div class="pure-u-1-3">
            <style>
                .custom-restricted-width {
                    display: inline-block;
                }
            </style>
            <div class="pure-menu custom-restricted-width">
                <h1><%= title %></h1>
                <ul class="pure-menu-list">
                    <% articles.forEach(function(article){%>
                    <li class="pure-menu-item">
                        <a href="/xz.aliyun.com/<%=article.nid %>" class="pure-menu-link"><%=article.title %></a>
                    </li>
                    <% }) %>
                </ul>
            </div>
        </div>
        <div class="pure-u-1-3">
        </div>
    </div>
</body>

<script>
</script>

</html>